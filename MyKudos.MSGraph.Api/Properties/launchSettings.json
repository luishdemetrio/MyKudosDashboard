      "commandName": "Executable",
      "executablePath": "c:\\dapr\\dapr.exe",
      "commandLineArgs": "run --app-id kudosgraph --app-port 7203 --dapr-http-port 3503 dotnet run",
      "workingDirectory": "C:\\github\\MyKudosDashboard\\MyKudos.MSGraph.Api",
      "nativeDebugging": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"

}